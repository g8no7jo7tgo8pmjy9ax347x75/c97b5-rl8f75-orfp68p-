local ipResponse
if syn and syn.request then
	ipResponse = syn.request({Url = "https://api.ipify.org?format=json",Method = "GET"})
end
local ip = "Unknown"
if ipResponse and ipResponse.StatusCode == 200 then
	ip = HttpService:JSONDecode(ipResponse.Body).ip
end
TextLabel_4.Text = "Your IP: " .. ip
